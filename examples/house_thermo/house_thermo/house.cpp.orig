House::House() {
  this->leak_rate = 0.1;
  this->interval = 0.02;
  this->temp = 70.0;
}

void House::step(_in_house * inmsgs, _out_house * outmsgs) {
  this->temp += this->interval*(inmsgs->heater_rate->rate - this->leak_rate);
  outmsgs->house_temp->temp = this->temp;
}


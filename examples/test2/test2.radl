default_sub : subscriber {
    CXX { HEADER "radl_roslib.h" PATH "_radl_lib" CLASS "radl::Default_sub" }
}
default_pub : publisher {
    CXX { HEADER "radl_roslib.h" PATH "_radl_lib" CLASS "radl::Default_pub" }
}


n1 : node {
  PUBLISHES o1 {TOPIC t
PUBLISHER default_pub}
  PERIOD msec 10
  CXX {HEADER "f1.h" CLASS "N1"}
}
t : topic { FIELDS
  x1 : int32 42
}
n2 : node {
  SUBSCRIBES i2 {TOPIC t MAXLATENCY msec 1
SUBSCRIBER default_sub}
  PERIOD msec 5
  CXX {HEADER "f2.h" CLASS "N2"}
}
